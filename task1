    </Div>
    <script type="text/javascript">
  
  var calculator = {
    read: function() {
        this.a = parseFloat(prompt("Введіть перше число:"));
        this.b = parseFloat(prompt("Введіть друге число:"));
    },
    sum: function() {
        return this.a + this.b;
    },
    mul: function() {
        return this.a * this.b;
    }
};

calculator.read();
console.log(calculator.sum());
console.log(calculator.mul());



        </script>
</body>
